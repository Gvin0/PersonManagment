@model int

@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found : </h4>
    <h5>
        Person with Id = @Model cannot be found
    </h5>
</div>

<a asp-action="Index" asp-controller="Person" class="btn btn-outline-success" style="width:auto">
    Click here to see list of all persons
</a>

